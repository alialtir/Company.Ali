@model Department


@{
    ViewData["Title"] = "Details";
}


<h1>Details Page</h1>

<br />
<br />

<form asp-action="Details">
    <div class="my-3">
        <label asp-for="Code" class="col-form-label"></label>
        <input asp-for="Code" class="form-control"  disabled />
    </div>

    <div class="my-3">
        <label asp-for="Name" class="col-form-label"></label>
        <input asp-for="Name" class="form-control" disabled />

    </div>

    <div class="my-3">
        <label asp-for="CreateAt" class="col-form-label"></label>
        <input asp-for="CreateAt" class="form-control" disabled />

    </div>

    <div class="my-3">
        <a asp-action="Update" class="btn btn-warning">Edit</a>
        <a asp-action="Index" class="btn btn-light">Back To List</a>
    </div>
</form>

